<h2>New Destination</h2>
<%= form_for(@destination) do |f| %>
  <p>
    <%= f.label :city %>
    <%= f.text_field :city %>
  </p>

  <p>
    <%= f.label :country %>
    <%= f.text_field :country %>
  </p>

  <p>
    <%= f.label :startdate %>
    <%= f.datetime_select :startdate %>
  </p>

  <p>
    <%= f.label :enddate %>
    <%= f.datetime_select :enddate %>
  </p>

  <%= f.submit %>
<% end %>
